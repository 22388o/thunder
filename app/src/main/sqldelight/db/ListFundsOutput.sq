CREATE TABLE ListFundsOutput (
    txid TEXT NOT NULL,
    output INTEGER NOT NULL DEFAULT 0,
    amountMsat INTEGER NOT NULL DEFAULT 0,
    scriptPubKey TEXT NOT NULL DEFAULT '',
    status TEXT NOT NULL DEFAULT '',
    reserved INTEGER AS Boolean NOT NULL DEFAULT 0,
    address TEXT,
    redeemScript TEXT,
    blockHeight INTEGER
);

insert:
INSERT INTO ListFundsOutput VALUES ?;

selectAll:
SELECT * FROM ListFundsOutput;

deleteAll:
DELETE FROM ListFundsOutput;